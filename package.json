    "scripts": {
        "ng": "ng",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch",
        "cd:root": "cd ../",
        "cd:android": "cd ./android",
        "cd:electron": "cd ./electron",
        "add:android": "npx cap add android",
        "open:android": "ng build && npx cap copy android && npx cap open android",
        "build-web:android": "ng build && npx cap copy android",
        "copy:android": "npx cap copy android",
        "watch-web:android": "ng build --watch --output-path ./android/app/src/main/assets/public",
        "build:android": "ng build && npx cap copy android && npm run cd:android && ./gradlew assembleDebug && npm run cd:root",
        "start:android": "cd android && gradlew assembleDebug && gradlew installDebug ",
        "add:electron": "npx cap add electron",
        "build:electron": "ng build && npx cap copy electron",
        "start:electron": "ng build && npx cap copy electron && npx cap open electron",
        "package:electron": "npm run build:electron && npm run cd:electron && npm run electron:package && npm run cd:root"
    },

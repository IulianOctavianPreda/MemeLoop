{
    "name": "capacitor-app",
    "version": "1.0.0",
    "description": "An Amazing Capacitor App",
    "main": "index.js",
    "scripts": {
        "electron:start": "electron ./",
        "electron:package": "electron-builder build ",
        "electron:package1": "electron-builder build --win -c.extraMetadata.main=./electron/index.js --publish never"
    },
    "dependencies": {
        "@capacitor/electron": "^1.5.2",
        "electron-is-dev": "^1.1.0"
    },

        "electron": "^9.4.0",
        "electron-builder": "^22.5.1"
    },
    "keywords": [
        "capacitor",
        "electron"
    ],
    "author": "",
    "license": "ISC",
    "build": {
        "productName": "PVM Player",
        "appId": "scripter.PVMplayer",
        "win": {
            "target": [
                "portable"
            ]
        },
        "portable": {
            "artifactName": "pvmplayer_portable.exe"
        }
    }
